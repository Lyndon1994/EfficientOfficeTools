{
   "background": {
      "persistent": false,
      "scripts": [
         "baidu.js"
      ]
   },
   "description": "快捷键切换百度搜索和Google搜索；在右键菜单添加百度文字搜索功能。",
   "homepage_url": "https://www.baidu.com",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "百度助手",
   "permissions": [
      "contextMenus",
      "activeTab"
   ],
   "short_name": "百度助手",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0",
   "commands": {
      "toggle-search": {
         "suggested_key": {
            "default": "Alt+S",
            "mac": "MacCtrl+S"
         },
         "description": "Toggle Search in Baidu and Google"
      }
   }
}